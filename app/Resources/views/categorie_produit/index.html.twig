{% extends 'base.html.twig' %}

{% block body %}
    <h1>Categorie_produits list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nomcategorie</th>
                <th>Typecategorie</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for categorie_Produit in categorie_Produits %}
            <tr>
                <td><a href="{{ path('categorie_produit_show', { 'id': categorie_Produit.id }) }}">{{ categorie_Produit.id }}</a></td>
                <td>{{ categorie_Produit.nomCategorie }}</td>
                <td>{{ categorie_Produit.typeCategorie }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('categorie_produit_show', { 'id': categorie_Produit.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('categorie_produit_edit', { 'id': categorie_Produit.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('categorie_produit_new') }}">Create a new categorie_Produit</a>
        </li>
    </ul>
{% endblock %}
